{
  "name": "express-type-safe-routes",
  "version": "0.1.0",
  "description": "Type-safe Express routes with Zod-powered inference for params, query, body, and responses.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "tsd",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0",
    "zod": "^3.20.0",
    "express-middleware-chain": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "express-middleware-chain": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/express": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.3",
    "express": "^4.19.2",
    "express-middleware-chain": "^0.1.0",
    "supertest": "^7.0.0",
    "tsd": "^0.30.0",
    "tsup": "^8.1.0",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "tsd": {
    "directory": "test/types"
  }
}
